{
  "manifest_version": 2,

  "name": "dewDrop",
  "description": "Trust Support Vouch for people in your social network.",
  "version": "1.0",

  "permissions": [
    "https://secure.flickr.com/",
    "https://www.facebook.com/*",
    "http://www.facebook.com/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}